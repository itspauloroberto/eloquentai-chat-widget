{
  "name": "@itspauloroberto/eloquentai-chat-widget",
  "version": "1.0.5",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/itspauloroberto/eloquentai-chat-widget.git"
